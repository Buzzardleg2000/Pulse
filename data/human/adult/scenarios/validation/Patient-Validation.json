{
  "Scenario": {
    "Name": "System-Validation",
    "Description": "Validation of all system data.",
    "PatientConfiguration": { "PatientFile": "StandardMale.json" },
    "DataRequestManager":
    {
      "DataRequest": [
        { "Category": "Patient",    "PropertyName": "Age",                               "Unit": "yr",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "Weight",                            "Unit": "kg",       "DecimalFormat": { "Precision": 1 } },
        { "Category": "Patient",    "PropertyName": "Height",                            "Unit": "in",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "BodyDensity",                       "Unit": "g/cm^3",   "DecimalFormat": { "Precision": 1 } },
        { "Category": "Patient",    "PropertyName": "BodyFatFraction",                                       "DecimalFormat": { "Precision": 3 } },
        { "Category": "Patient",    "PropertyName": "BodyMassIndex",                                         "DecimalFormat": { "Precision": 3 } },
        { "Category": "Patient",    "PropertyName": "LeanBodyMass",                      "Unit": "kg",       "DecimalFormat": { "Precision": 1 } },
        { "Category": "Patient",    "PropertyName": "IdealBodyWeight",                   "Unit": "kg",       "DecimalFormat": { "Precision": 1 } },
        { "Category": "Patient",    "PropertyName": "AlveoliSurfaceArea",                "Unit": "m^2",      "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "RightLungRatio",                                        "DecimalFormat": { "Precision": 2 } },
        { "Category": "Patient",    "PropertyName": "SkinSurfaceArea",                   "Unit": "m^2",      "DecimalFormat": { "Precision": 2 } },
        { "Category": "Patient",    "PropertyName": "BasalMetabolicRate",                "Unit": "kcal/day", "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "BloodVolumeBaseline",               "Unit": "mL",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "DiastolicArterialPressureBaseline", "Unit": "mmHg",     "DecimalFormat": { "Precision": 4 } },
        { "Category": "Patient",    "PropertyName": "HeartRateBaseline",                 "Unit": "1/min",    "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "MeanArterialPressureBaseline",      "Unit": "mmHg",     "DecimalFormat": { "Precision": 4 } },
        { "Category": "Patient",    "PropertyName": "PulsePressureBaseline",             "Unit": "mmHg",     "DecimalFormat": { "Precision": 4 } },
        { "Category": "Patient",    "PropertyName": "RespirationRateBaseline",           "Unit": "1/min",    "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "SystolicArterialPressureBaseline",  "Unit": "mmHg",     "DecimalFormat": { "Precision": 4 } },
        { "Category": "Patient",    "PropertyName": "TidalVolumeBaseline",               "Unit": "mL",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "HeartRateMaximum",                  "Unit": "1/min",    "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "HeartRateMinimum",                  "Unit": "1/min",    "DecimalFormat": { "Precision": 3 } },
        { "Category": "Patient",    "PropertyName": "ExpiratoryReserveVolume",           "Unit": "mL",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "FunctionalResidualCapacity",        "Unit": "mL",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "InspiratoryCapacity",               "Unit": "mL",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "InspiratoryReserveVolume",          "Unit": "mL",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "ResidualVolume",                    "Unit": "mL",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "TotalLungCapacity",                 "Unit": "mL",       "DecimalFormat": {                } },
        { "Category": "Patient",    "PropertyName": "VitalCapacity",                     "Unit": "mL",       "DecimalFormat": {                } }
      ]
    },
    "DataRequestFile":
    [
      "./validation/requests/BloodChemistry.json",
      "./validation/requests/Cardiovascular.json",
      "./validation/requests/CardiovascularCompartments.json",
      "./validation/requests/Endocrine.json",
      "./validation/requests/Energy.json",
      "./validation/requests/Gastrointestinal.json",
      "./validation/requests/Renal.json",
      "./validation/requests/RenalCompartments.json",
      "./validation/requests/RenalSubstances.json",
      "./validation/requests/Respiratory.json",
      "./validation/requests/RespiratoryCompartments.json",
      "./validation/requests/Tissue.json",
      "./validation/requests/TissueCompartments.json",
      "./validation/requests/TissueSubstances.json"
    ],
    "AnyAction": 
    [
      { "AdvanceTime": { "Time": { "ScalarTime": { "Value": 2.0, "Unit": "min" }}}},
      { "PatientAction": { "Assessment": { "Type": "ArterialBloodGasTest" }}},
      { "PatientAction": { "Assessment": { "Type": "CompleteBloodCount" }}},
      { "PatientAction": { "Assessment": { "Type": "ComprehensiveMetabolicPanel" }}},
      { "PatientAction": { "Assessment": { "Type": "Urinalysis" }}}
    ]
  }
}